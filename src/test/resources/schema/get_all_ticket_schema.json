{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/Welcome",
  "definitions": {
    "Welcome": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "status": {
          "type": "boolean"
        },
        "message": {
          "type": "string"
        },
        "data": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Datum"
          }
        }
      },
      "required": [
        "data",
        "message",
        "status"
      ],
      "title": "Welcome"
    },
    "Datum": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "integer"
        },
        "type": {
          "$ref": "#/definitions/Type"
        },
        "price": {
          "type": "integer"
        },
        "qty": {
          "type": "integer"
        },
        "name": {
          "$ref": "#/definitions/Name"
        },
        "description": {
          "$ref": "#/definitions/DescriptionUnion"
        },
        "startTime": {
          "type": "string",
          "format": "date-time"
        },
        "endTime": {
          "type": "string",
          "format": "date-time"
        },
        "eventId": {
          "type": "integer"
        },
        "event": {
          "$ref": "#/definitions/Event"
        }
      },
      "required": [
        "description",
        "endTime",
        "event",
        "eventId",
        "id",
        "name",
        "price",
        "qty",
        "startTime",
        "type"
      ],
      "title": "Datum"
    },
    "Event": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "integer"
        },
        "fromDate": {
          "type": "string",
          "format": "date-time"
        },
        "toDate": {
          "type": "string",
          "format": "date-time"
        },
        "eventName": {
          "$ref": "#/definitions/EventName"
        },
        "eventDescription": {
          "$ref": "#/definitions/EventDescription"
        },
        "addressId": {
          "type": "null"
        },
        "creatorId": {
          "type": "integer"
        },
        "categoryId": {
          "type": "integer"
        }
      },
      "required": [
        "addressId",
        "categoryId",
        "creatorId",
        "eventDescription",
        "eventName",
        "fromDate",
        "id",
        "toDate"
      ],
      "title": "Event"
    },
    "DescriptionUnion": {
      "anyOf": [
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "$ref": "#/definitions/DescriptionEnum"
        }
      ],
      "title": "DescriptionUnion"
    },
    "DescriptionEnum": {
      "type": "string",
      "enum": [
        "",
        "this is band noah",
        "konser jkt48"
      ],
      "title": "DescriptionEnum"
    },
    "EventDescription": {
      "type": "string",
      "enum": [
        "This is taylor swift"
      ],
      "title": "EventDescription"
    },
    "EventName": {
      "type": "string",
      "enum": [
        "Taylor Swift"
      ],
      "title": "EventName"
    },
    "Name": {
      "type": "string",
      "enum": [
        "",
        "Noah",
        "JKT48"
      ],
      "title": "Name"
    },
    "Type": {
      "type": "string",
      "enum": [
        "VIP",
        "Regular",
        "REGULER"
      ],
      "title": "Type"
    }
  }
}
